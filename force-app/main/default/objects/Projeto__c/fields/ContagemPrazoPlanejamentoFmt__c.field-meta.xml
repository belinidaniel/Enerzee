<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContagemPrazoPlanejamentoFmt__c</fullName>
    <description>Tempo em Planejamento de projeto formatado (dias, horas, minutos, segundos).</description>
    <externalId>false</externalId>
    <formula>IF(
    ISBLANK(ContagemPrazoPlanejamento__c),
    NULL,
    TEXT(FLOOR(ContagemPrazoPlanejamento__c)) &amp; &quot;d &quot; &
    TEXT(FLOOR(MOD(ContagemPrazoPlanejamento__c * 24, 24))) &amp; &quot;h &quot; &
    TEXT(FLOOR(MOD(ContagemPrazoPlanejamento__c * 24 * 60, 60))) &amp; &quot;m &quot; &
    TEXT(FLOOR(MOD(ContagemPrazoPlanejamento__c * 24 * 60 * 60, 60))) &amp; &quot;s&quot;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Planejamento de projeto</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
