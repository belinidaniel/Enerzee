<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Endereco_obrigatorio</fullName>
    <active>false</active>
    <description>O Endereço é obrigatório.</description>
    <errorConditionFormula>ISBLANK(Address ) ||
				 ISNULL(Address)</errorConditionFormula>
    <errorMessage>Endereço Obrigatório.</errorMessage>
</ValidationRule>
